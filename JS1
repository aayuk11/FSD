       let marks=7;
       if(marks>=90){
              console.log('A');
       }
       else if(marks>=80){
              console.log('B');
       }
       else if(marks>=70){
              console.log('C');
       }
       else if(marks>=60){
              console.log('D');
       }
       else{
              console.log('E');
       }

       let num=8;
       switch(num){
              case 1:console.log('A')
              break;
              case 2:console.log('B');
              break;
              case 3:console.log('C');
              break;
              default:console.log('D');
              break;
       }

       for(i=1; i<10; i++){
