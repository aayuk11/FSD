       let c=7;
       let d=8;
      // console.log(c**d);
       //console.log(c+d);
       let ans1 = (++c)*(--d);
       let ans2 = (++c)*(d--);
       let ans3 = (c++)*(d++);
       let ans4 = (--c)*(d--);
       console.log(ans1);
       console.log(ans2);
       console.log(ans3);
       console.log(ans4);
       console.log(c);
       console.log(d);

